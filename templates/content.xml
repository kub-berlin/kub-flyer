              <text:p text:style-name="Box"><text:span text:style-name="BoxBold">{{ Anmeldung }}</text:span></text:p>
              <text:p text:style-name="Box"><text:span text:style-name="Box">{{ Wenn Sie eine Beratung brauchen: }}</text:span></text:p>
              <text:p text:style-name="Box"><text:span text:style-name="Box">{{ Rufen Sie uns bitte an: }} &#8296;030 / 614 94 00&#8297;</text:span></text:p>
              <text:p text:style-name="Box"><text:span text:style-name="Box">{{ Oder schreiben Sie uns eine E-Mail: }} frauenberatung@kub-berlin.org</text:span></text:p>
            </draw:text-box>
